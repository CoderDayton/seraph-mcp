{
    "$schema": "https://gofastmcp.com/public/schemas/fastmcp.json/v1.json",
    "source": {
        "type": "filesystem",
        "path": "src/server.py",
        "entrypoint": "mcp"
    },
    "environment": {
        "type": "uv",
        "python": ">=3.12",
        "dependencies": [
            "fastmcp>=2.0.0",
            "pydantic>=2.0.0",
            "pydantic-settings>=2.0.0",
            "python-dotenv>=1.0.0",
            "redis>=5.0.0",
            "httpx>=0.25.0",
            "openai>=1.0.0",
            "anthropic>=0.25.0",
            "google-genai>=0.2.0",
            "tiktoken>=0.5.0",
            "llmlingua>=0.2.2",
            "langchain>=0.3.27",
            "blake3>=1.0.7"
        ],
        "editable": ["."]
    },
    "deployment": {
        "transport": "stdio",
        "log_level": "WARNING"
    }
}
